<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane
    xmlns:fx="http://javafx.com/fxml/1" 
    id="grid"
    prefHeight="400.0"
    prefWidth="1000.0"
    alignment="CENTER"
    fx:controller="geoconversor.controller.AppController"
>
    <stylesheets>
        <URL value="@AppStyleSheet.css" />
    </stylesheets>
    
    <ScrollPane 
        GridPane.columnIndex="0"                
        GridPane.rowIndex="0"
        GridPane.rowSpan="3">
        
        <content >
            <fx:include source="list.fxml"/>
        </content>
        
    </ScrollPane>   
    
    <ScrollPane 
        GridPane.columnIndex="1" 
        GridPane.rowIndex="0"
        GridPane.rowSpan="3"> 
        
        <content >            
            <fx:include source="properties.fxml"/>
        </content>

    </ScrollPane> 

    <ScrollPane 
        GridPane.columnIndex="2"
        GridPane.rowIndex="0">
        
        <content >
            <fx:include source="utm_convert.fxml"/>
        </content>
    </ScrollPane>  
    
    <ScrollPane 
        GridPane.columnIndex="3"
        GridPane.rowIndex="0">        
        <content >
            <fx:include source="latlon_convert.fxml"/>
        </content>
    </ScrollPane>  
    
    <ScrollPane 
        GridPane.columnIndex="4"
        GridPane.rowIndex="0">        
        <content >
            <fx:include source="dms_convert.fxml"/>
        </content>
    </ScrollPane>                 
    
</GridPane>